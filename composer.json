{
    "name": "yusukezzz/slim-debugbar",
    "license": "MIT",
    "keywords": ["slim", "middleware", "debugbar"],
    "require": {
        "php": ">=5.4.0",
        "slim/slim": "2.*",
        "maximebf/debugbar": ">=1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "twig/twig": "1.*",
        "slim/views": "0.1.*"
    },
    "suggest": {
        "twig/twig": "DebugBar has data collector for this template",
        "slim/views": "Smarty and Twig custom view classes for Slim framework"
    },
    "autoload": {
        "psr-0": {
            "Slim\\Middleware\\": "src/",
            "DebugBar\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
